{
  "compilerOptions": {
    "target": "es6",
    "module": "ES6",
    "outDir": "./dist",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "express": ["node_modules/@types/express/index"],
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules/**/*"]
}
